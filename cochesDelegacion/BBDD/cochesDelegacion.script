CREATE TRIGGER PUBLIC.CONTROLHORAS BEFORE INSERT ON PUBLIC.CONDUCEN REFERENCING NEW ROW AS NEWROW FOR EACH ROW BEGIN ATOMIC DECLARE ENTREGA TIMESTAMP;SET(ENTREGA)=(SELECT MAX(HORAENTREGA)FROM PUBLIC.CONDUCEN WHERE MATRICULA=NEWROW.MATRICULA AND FECHARECOGIDA=NEWROW.FECHARECOGIDA);IF(NEWROW.HORARECOGIDA<ENTREGA)THEN SIGNAL SQLSTATE'45000'SET MESSAGE_TEXT='El coche esta ocupado a la hora que lo pretendes recoger, elige otra hora u otro coche.';END IF;END
