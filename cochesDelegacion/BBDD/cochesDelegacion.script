CREATE MEMORY TABLE PUBLIC.USUARIOS(USERWIN VARCHAR(15),NOMBRE VARCHAR(50),APELLIDOS VARCHAR(50),CORREO VARCHAR(150),TELEFONO INTEGER,CONSTRAINT USR_USW_PK PRIMARY KEY(USERWIN))
CREATE MEMORY TABLE PUBLIC.ACCESOUSER(USERWIN VARCHAR(15),PASS VARCHAR(25),TIPO VARCHAR(10),CONSTRAINT ACC_USW_PK PRIMARY KEY(USERWIN),CONSTRAINT ACC_USW_FK FOREIGN KEY(USERWIN) REFERENCES PUBLIC.USUARIOS(USERWIN) ON DELETE CASCADE)
CREATE MEMORY TABLE PUBLIC.COCHES(MARCA VARCHAR(25),MODELO VARCHAR(25),MATRICULA VARCHAR(10),CONSTRAINT CCH_MAT_PK PRIMARY KEY(MATRICULA))
CREATE MEMORY TABLE PUBLIC.CONDUCEN(USERWIN VARCHAR(15),MATRICULA VARCHAR(10),FECHARECOGIDA DATE,HORARECOGIDA TIMESTAMP,HORAENTREGA TIMESTAMP,MOTIVO VARCHAR(100),CONSTRAINT CND_UMFHI_PK PRIMARY KEY(USERWIN,MATRICULA,FECHARECOGIDA,HORARECOGIDA),CONSTRAINT CND_USW_FK FOREIGN KEY(USERWIN) REFERENCES PUBLIC.USUARIOS(USERWIN) ON DELETE CASCADE,CONSTRAINT CND_MAT_FK FOREIGN KEY(MATRICULA) REFERENCES PUBLIC.COCHES(MATRICULA) ON DELETE CASCADE,CONSTRAINT CND_FRMFE_CHK CHECK(PUBLIC.CONDUCEN.HORAENTREGA>PUBLIC.CONDUCEN.HORARECOGIDA))
CREATE MEMORY TABLE PUBLIC.PRUEBA(LA TIMESTAMP)
CREATE PROCEDURE PUBLIC.RECUPERAR_USUARIO(IN USW VARCHAR(15),IN PAS VARCHAR(15),OUT USWO VARCHAR(15),OUT TIP VARCHAR(7)) SPECIFIC RECUPERAR_USUARIO_10159 LANGUAGE SQL NOT DETERMINISTIC READS SQL DATA NEW SAVEPOINT LEVEL BEGIN ATOMIC SELECT USERWIN,TIPO INTO USWO,TIP FROM PUBLIC.ACCESOUSER WHERE USERWIN=USW AND PASS=PAS;END
CREATE PROCEDURE PUBLIC.INSERTARFUNCIONARIO(IN USERWIN VARCHAR(15),IN NOMBRE VARCHAR(50),IN APELLIDOS VARCHAR(50),IN CORREO VARCHAR(150),IN TELEFONO INTEGER) SPECIFIC INSERTARFUNCIONARIO_10163 LANGUAGE SQL NOT DETERMINISTIC MODIFIES SQL DATA NEW SAVEPOINT LEVEL BEGIN ATOMIC INSERT INTO PUBLIC.USUARIOS(USERWIN,NOMBRE,APELLIDOS,CORREO,TELEFONO)VALUES(USERWIN,NOMBRE,APELLIDOS,CORREO,TELEFONO);END
CREATE PROCEDURE PUBLIC.INSERTUSER(IN USERWIN VARCHAR(15),IN PASS VARCHAR(25),IN TIPO VARCHAR(10)) SPECIFIC INSERTUSER_10167 LANGUAGE SQL NOT DETERMINISTIC MODIFIES SQL DATA NEW SAVEPOINT LEVEL BEGIN ATOMIC INSERT INTO PUBLIC.ACCESOUSER(USERWIN,PASS,TIPO)VALUES(USERWIN,PASS,TIPO);END
CREATE PROCEDURE PUBLIC.MODUFICARFUNCIONARIO(IN USW VARCHAR(15),IN NMB VARCHAR(50),IN APDS VARCHAR(50),IN CRR VARCHAR(150),IN TLF INTEGER) SPECIFIC MODUFICARFUNCIONARIO_10171 LANGUAGE SQL NOT DETERMINISTIC MODIFIES SQL DATA NEW SAVEPOINT LEVEL BEGIN ATOMIC UPDATE PUBLIC.USUARIOS SET NOMBRE=NMB,APELLIDOS=APDS,CORREO=CRR,TELEFONO=TLF WHERE USERWIN=USW;END
CREATE PROCEDURE PUBLIC.BORRARFUNCIONARIO(IN USW VARCHAR(15)) SPECIFIC BORRARFUNCIONARIO_10179 LANGUAGE SQL NOT DETERMINISTIC MODIFIES SQL DATA NEW SAVEPOINT LEVEL BEGIN ATOMIC DELETE FROM PUBLIC.USUARIOS WHERE USERWIN=USW;END
CREATE PROCEDURE PUBLIC.BORRARUSUARIO(IN USW VARCHAR(15)) SPECIFIC BORRARUSUARIO_10183 LANGUAGE SQL NOT DETERMINISTIC MODIFIES SQL DATA NEW SAVEPOINT LEVEL BEGIN ATOMIC DELETE FROM PUBLIC.ACCESOUSER WHERE USERWIN=USW;END
CREATE PROCEDURE PUBLIC.MODIFICARUSUARIO(IN USW VARCHAR(15),IN PSSO VARCHAR(15),IN PSS VARCHAR(15)) SPECIFIC MODIFICARUSUARIO_10114 LANGUAGE SQL NOT DETERMINISTIC MODIFIES SQL DATA NEW SAVEPOINT LEVEL BEGIN ATOMIC UPDATE PUBLIC.ACCESOUSER SET PASS=PSS WHERE USERWIN=USW AND PASS=PSSO;END
CREATE PROCEDURE PUBLIC.INSERTCOCHE(IN MARCA VARCHAR(25),IN MODELO VARCHAR(25),IN MATRICULA VARCHAR(10)) SPECIFIC INSERTCOCHE_10111 LANGUAGE SQL NOT DETERMINISTIC MODIFIES SQL DATA NEW SAVEPOINT LEVEL BEGIN ATOMIC INSERT INTO PUBLIC.COCHES(MARCA,MODELO,MATRICULA)VALUES(MARCA,MODELO,MATRICULA);END
CREATE PROCEDURE PUBLIC.MODIFICARCOCHE(IN MAR VARCHAR(15),IN MDE VARCHAR(15),IN MAT VARCHAR(15)) SPECIFIC MODIFICARCOCHE_10114 LANGUAGE SQL NOT DETERMINISTIC MODIFIES SQL DATA NEW SAVEPOINT LEVEL BEGIN ATOMIC UPDATE PUBLIC.COCHES SET MARCA=MAR,MODELO=MDE WHERE MATRICULA=MAT;END
CREATE PROCEDURE PUBLIC.BORRARCOCHE(IN MAT VARCHAR(15)) SPECIFIC BORRARCOCHE_10128 LANGUAGE SQL NOT DETERMINISTIC MODIFIES SQL DATA NEW SAVEPOINT LEVEL BEGIN ATOMIC DELETE FROM PUBLIC.COCHES WHERE MATRICULA=MAT;END
CREATE PROCEDURE PUBLIC.CARGAUSUARIO(IN USWI VARCHAR(150),IN PASSI VARCHAR(150),OUT USWO VARCHAR(150),OUT TIP VARCHAR(150),OUT NOM VARCHAR(150),OUT APO VARCHAR(150),OUT CORRO VARCHAR(150),OUT TLFO INTEGER) SPECIFIC CARGAUSUARIO_10116 LANGUAGE SQL NOT DETERMINISTIC READS SQL DATA NEW SAVEPOINT LEVEL BEGIN ATOMIC SELECT A.USERWIN,A.TIPO,U.NOMBRE,U.APELLIDOS,U.CORREO,U.TELEFONO INTO USWO,TIP,NOM,APO,CORRO,TLFO FROM PUBLIC.ACCESOUSER A JOIN PUBLIC.USUARIOS U ON A.USERWIN=U.USERWIN WHERE USERWIN=USWI AND PASS=PASSI;END
CREATE PROCEDURE PUBLIC.INSERTCONDUCEN(IN USERWIN VARCHAR(15),IN MATRICULA VARCHAR(10),IN FECHARECOGIDA DATE,IN HORARECOGIDA TIMESTAMP,IN HORAENTREGA TIMESTAMP,IN MOTIVO VARCHAR(100)) SPECIFIC INSERTCONDUCEN_10112 LANGUAGE SQL NOT DETERMINISTIC MODIFIES SQL DATA NEW SAVEPOINT LEVEL BEGIN ATOMIC INSERT INTO PUBLIC.CONDUCEN(USERWIN,MATRICULA,FECHARECOGIDA,HORARECOGIDA,HORAENTREGA,MOTIVO)VALUES(USERWIN,MATRICULA,FECHARECOGIDA,HORARECOGIDA,HORAENTREGA,MOTIVO);END
CREATE PROCEDURE PUBLIC.CARGACOCHES() SPECIFIC CARGACOCHES_10132 LANGUAGE SQL NOT DETERMINISTIC READS SQL DATA NEW SAVEPOINT LEVEL  DYNAMIC RESULT SETS 1 BEGIN ATOMIC DECLARE RESULT CURSOR FOR SELECT PUBLIC.COCHES.MARCA,PUBLIC.COCHES.MODELO,PUBLIC.COCHES.MATRICULA FROM PUBLIC.COCHES ORDER BY MATRICULA ASC;OPEN RESULT;END
CREATE PROCEDURE PUBLIC.CARGARESERVAS(IN FECHARI VARCHAR(50)) SPECIFIC CARGARESERVAS_10122 LANGUAGE SQL NOT DETERMINISTIC READS SQL DATA NEW SAVEPOINT LEVEL  DYNAMIC RESULT SETS 1 BEGIN ATOMIC DECLARE RESULT CURSOR FOR SELECT A.USERWIN,U.NOMBRE,U.APELLIDOS,CO.MARCA,CO.MODELO,C.FECHARECOGIDA,C.HORARECOGIDA,C.HORAENTREGA,C.MOTIVO FROM PUBLIC.ACCESOUSER A JOIN PUBLIC.USUARIOS U ON A.USERWIN=U.USERWIN JOIN PUBLIC.CONDUCEN C ON C.USERWIN=A.USERWIN JOIN PUBLIC.COCHES CO ON CO.MATRICULA=C.MATRICULA WHERE C.FECHARECOGIDA=FECHARI;OPEN RESULT;END
CREATE PROCEDURE PUBLIC.CARGARESERVAS1() SPECIFIC CARGARESERVAS1_10122 LANGUAGE SQL NOT DETERMINISTIC READS SQL DATA NEW SAVEPOINT LEVEL  DYNAMIC RESULT SETS 1 BEGIN ATOMIC DECLARE RESULT CURSOR FOR SELECT A.USERWIN,U.NOMBRE,U.APELLIDOS,CO.MARCA,CO.MODELO,C.FECHARECOGIDA,C.HORARECOGIDA,C.HORAENTREGA,C.MOTIVO FROM PUBLIC.ACCESOUSER A JOIN PUBLIC.USUARIOS U ON A.USERWIN=U.USERWIN JOIN PUBLIC.CONDUCEN C ON C.USERWIN=A.USERWIN JOIN PUBLIC.COCHES CO ON CO.MATRICULA=C.MATRICULA ORDER BY C.FECHARECOGIDA;OPEN RESULT;END
