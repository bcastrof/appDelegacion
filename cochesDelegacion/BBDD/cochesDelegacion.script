CREATE MEMORY TABLE PUBLIC.CONDUCEN(USERWIN VARCHAR(15),MATRICULA VARCHAR(10),FECHARECOGIDA DATE,HORARECOGIDA TIMESTAMP,HORAENTREGA TIMESTAMP,MOTIVO VARCHAR(100),CONSTRAINT CND_UMFHI_PK PRIMARY KEY(USERWIN,MATRICULA,FECHARECOGIDA,HORARECOGIDA),CONSTRAINT CND_USW_FK FOREIGN KEY(USERWIN) REFERENCES PUBLIC.USUARIOS(USERWIN) ON DELETE SET NULL,CONSTRAINT CND_MAT_FK FOREIGN KEY(MATRICULA) REFERENCES PUBLIC.COCHES(MATRICULA) ON DELETE SET NULL,CONSTRAINT CND_FRMFE_CHK CHECK(PUBLIC.CONDUCEN.HORAENTREGA>PUBLIC.CONDUCEN.HORARECOGIDA))
CREATE PROCEDURE PUBLIC.RECUPERARUSUARIO(IN USW VARCHAR(15),IN PAS VARCHAR(25)) SPECIFIC RECUPERARUSUARIO_10131 LANGUAGE SQL NOT DETERMINISTIC READS SQL DATA NEW SAVEPOINT LEVEL  DYNAMIC RESULT SETS 1 BEGIN ATOMIC DECLARE RES CURSOR FOR SELECT PUBLIC.ACCESOUSER.USERWIN,PUBLIC.ACCESOUSER.PASS,PUBLIC.ACCESOUSER.TIPO FROM PUBLIC.ACCESOUSER WHERE USERWIN=USW AND PASS=PAS;OPEN RES;END
CREATE PROCEDURE PUBLIC.RECUPERARFUNCI(IN USW VARCHAR(15)) SPECIFIC RECUPERARFUNCI_10144 LANGUAGE SQL NOT DETERMINISTIC READS SQL DATA NEW SAVEPOINT LEVEL  DYNAMIC RESULT SETS 1 BEGIN ATOMIC DECLARE RES CURSOR FOR SELECT PUBLIC.USUARIOS.USERWIN,PUBLIC.USUARIOS.NOMBRE,PUBLIC.USUARIOS.APELLIDOS,PUBLIC.USUARIOS.CORREO,PUBLIC.USUARIOS.TELEFONO FROM PUBLIC.USUARIOS WHERE USERWIN=USW;OPEN RES;END
CREATE PROCEDURE PUBLIC.RECUPERARFUNCIONARIOS() SPECIFIC RECUPERARFUNCIONARIOS_10150 LANGUAGE SQL NOT DETERMINISTIC READS SQL DATA NEW SAVEPOINT LEVEL  DYNAMIC RESULT SETS 1 BEGIN ATOMIC DECLARE RES CURSOR FOR SELECT PUBLIC.USUARIOS.USERWIN,PUBLIC.USUARIOS.NOMBRE,PUBLIC.USUARIOS.APELLIDOS,PUBLIC.USUARIOS.CORREO,PUBLIC.USUARIOS.TELEFONO FROM PUBLIC.USUARIOS;OPEN RES;END
CREATE PROCEDURE PUBLIC.INSERTARFUNCIONARIO(IN USERWIN VARCHAR(15),IN NOMBRE VARCHAR(50),IN APELLIDOS VARCHAR(50),IN CORREO VARCHAR(150),IN TELEFONO INTEGER) SPECIFIC INSERTARFUNCIONARIO_10155 LANGUAGE SQL NOT DETERMINISTIC MODIFIES SQL DATA NEW SAVEPOINT LEVEL BEGIN ATOMIC INSERT INTO PUBLIC.USUARIOS(USERWIN,NOMBRE,APELLIDOS,CORREO,TELEFONO)VALUES(USERWIN,NOMBRE,APELLIDOS,CORREO,TELEFONO);END
CREATE PROCEDURE PUBLIC.INSERTUSER(IN USERWIN VARCHAR(15),IN PASS VARCHAR(25),IN TIPO VARCHAR(10)) SPECIFIC INSERTUSER_10164 LANGUAGE SQL NOT DETERMINISTIC MODIFIES SQL DATA NEW SAVEPOINT LEVEL BEGIN ATOMIC INSERT INTO PUBLIC.ACCESOUSER(USERWIN,PASS,TIPO)VALUES(USERWIN,PASS,TIPO);END
CREATE PROCEDURE PUBLIC.MODUFICARFUNCIONARIO(IN USW VARCHAR(15),IN NMB VARCHAR(50),IN APDS VARCHAR(50),IN CRR VARCHAR(150),IN TLF INTEGER) SPECIFIC MODUFICARFUNCIONARIO_10169 LANGUAGE SQL NOT DETERMINISTIC MODIFIES SQL DATA NEW SAVEPOINT LEVEL BEGIN ATOMIC UPDATE PUBLIC.USUARIOS SET NOMBRE=NMB,APELLIDOS=APDS,CORREO=CRR,TELEFONO=TLF WHERE USERWIN=USW;END
CREATE PROCEDURE PUBLIC.MODIFICARUSUARIO(IN USW VARCHAR(15),IN PSS VARCHAR(15)) SPECIFIC MODIFICARUSUARIO_10174 LANGUAGE SQL NOT DETERMINISTIC MODIFIES SQL DATA NEW SAVEPOINT LEVEL BEGIN ATOMIC UPDATE PUBLIC.ACCESOUSER SET PASS=PSS WHERE USERWIN=USW;END
CREATE PROCEDURE PUBLIC.BORRARFUNCIONARIO(IN USW VARCHAR(15)) SPECIFIC BORRARFUNCIONARIO_10178 LANGUAGE SQL NOT DETERMINISTIC MODIFIES SQL DATA NEW SAVEPOINT LEVEL BEGIN ATOMIC DELETE FROM PUBLIC.USUARIOS WHERE USERWIN=USW;END
CREATE PROCEDURE PUBLIC.BORRARUSUARIO(IN USW VARCHAR(15)) SPECIFIC BORRARUSUARIO_10182 LANGUAGE SQL NOT DETERMINISTIC MODIFIES SQL DATA NEW SAVEPOINT LEVEL BEGIN ATOMIC DELETE FROM PUBLIC.ACCESOUSER WHERE USERWIN=USW;END
CREATE PROCEDURE PUBLIC.RECUPERAR_USUARIO(IN USW VARCHAR(15),IN PAS VARCHAR(15),OUT USWO VARCHAR(15),OUT TIP VARCHAR(7)) SPECIFIC RECUPERAR_USUARIO_10117 LANGUAGE SQL NOT DETERMINISTIC READS SQL DATA NEW SAVEPOINT LEVEL BEGIN ATOMIC SELECT USERWIN,TIPO INTO USWO,TIP FROM PUBLIC.ACCESOUSER WHERE USERWIN=USW AND PASS=PAS;END
